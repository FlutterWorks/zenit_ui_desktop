    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/web-components": "^6.5.13",
    "@types/jest": "^27.0.3",
    "babel-loader": "^8.3.0",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "lit-html": "^2.4.0",
    "puppeteer": "^19.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^6.5.13",
    "storybook-addon-themes": "^6.1.0"
